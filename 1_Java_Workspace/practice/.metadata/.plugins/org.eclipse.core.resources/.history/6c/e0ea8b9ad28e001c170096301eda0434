// 3.?¸ìŠ¤?´ìŠ¤ ?Œì† ë©”ì†Œ??
class first {
	public String delimiter;
	public static void last1() {				 // last1() ì°¨ì´
		System.out.println("f1");				 // static O
	}
	public static void last2(String delimiter) { // last2() ì°¨ì´
		System.out.println(delimiter);			 // êµ¬ë¶„??delimiter
		System.out.println("f2");				 // static O
	}
	public void last3() {						 // last3() ì°¨ì´
		System.out.println(this.delimiter);		 // êµ¬ë¶„??delimiter
		System.out.println("f3");				 // static X
	}
}
public class practice_everyday001 {
	public static void main(String[] args) {
		
		first f1 = new first();		// ?¸ìŠ¤?´ìŠ¤ ?Œì† ë©”ì†Œ
		f1.last1();					// but, no delimiter & static O
		
		first f2 = new first(); 	// êµ¬ë¶„??ì£¼ë‹ˆê¹??‘ë™
		f2.last2("====");			// 
		
		first f3 = new first();		// êµ¬ë¶„???ˆì£¼ê³??‘ë™ ì¡°ê±´
		f3.last3();					// last3?ì„œ ()ì§??°ê³  static?? œ. this. ì¶”ê? ???‘ë™
//		f3.last3("$$$$");			// ?ëŸ¬
		f3.delimiter = "$$$$";		
		f3.last3();					// last3?ì„œ ()ì§??°ê³  static?? œ. this. ì¶”ê? ???‘ë™
		
	}
}
