

// 메소드
// 클래스 소속 메소드
// 인스턴스 소속 메소드


public class practice_everyday {
	public static void last1() {				 // () 차이
		System.out.println("메소드");
		System.out.println("메소드");
	}
	public static void last2(String delimiter) { // () 차이
		System.out.println(delimiter);
		System.out.println("메소드");
		System.out.println("메소드");
	}
	public static void main(String[] args) {
		
		last1();
//		last1("----"); 에러 : ()에 구분자가 없어야함
		last2("====");
		
		
	}
}
